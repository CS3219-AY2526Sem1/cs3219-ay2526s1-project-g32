{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es2020": true
  },
  "extends": ["eslint:recommended", "plugin:import/recommended", "prettier"],
  "rules": {
    "import/order": ["error", {"newlines-between": "always"}],
    "no-console": ["warn", {"allow": ["warn", "error"]}]
  }
}
